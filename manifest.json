  "name": "VJCHROME",
  "version": "1.0",
  "description": "OPEN_SOURCE1!",
  "manifest_version": 2,
  
  "icons": {
    "128": "assets/32.png"
  },
  "permissions":[
       "<all_urls>",
       "contextMenus",
       "tabs"
  ],
  "content_scripts": [{
    "run":"document_start",
